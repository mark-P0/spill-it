{
  "name": "spill-it",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "api:start": "npm run api:check && npm run api:preview",
    "api:check": "tsc",
    "api:preview": "cross-env NODE_ENV=production tsx ./api/bin/www",
    "api:dev": "cross-env NODE_ENV=development DEBUG=spill-it:* tsx --watch ./api/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "express-generator": "^4.16.1",
    "prettier": "^3.1.1"
  }
}
