{
  "name": "@spill-it/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm lint && pnpm check && pnpm preview",
    "lint": "eslint . --ext ts --max-warnings 0",
    "lint:imports": "pnpm lint --rule \"@typescript-eslint/no-unused-vars: warn\"",
    "check": "tsc",
    "preview": "cross-env NODE_ENV=production tsx --env-file=.env ./bin/www",
    "dev": "cross-env NODE_ENV=development tsx --env-file=.env --watch ./bin/www"
  },
  "dependencies": {
    "@spill-it/auth": "workspace:^",
    "@spill-it/db": "workspace:^",
    "@spill-it/endpoints": "workspace:^",
    "@spill-it/header-auth": "workspace:^",
    "@spill-it/utils": "workspace:^",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "~4.16.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.2.0",
    "morgan": "~1.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "express-generator": "^4.16.1",
    "prettier": "^3.1.1"
  }
}
