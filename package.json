{
  "name": "spill-it",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:migrate": "npm run db:generate-migrations && npm run db:execute-migrations",
    "db:generate-migrations": "cross-env NODE_ENV=development drizzle-kit generate:pg",
    "db:execute-migrations": "cross-env NODE_ENV=development tsx --env-file=.env ./api/data/migrate.ts",
    "api:start": "npm run api:check && npm run api:preview",
    "api:check": "tsc",
    "api:preview": "cross-env NODE_ENV=production tsx --env-file=.env ./api/bin/www",
    "api:dev": "cross-env NODE_ENV=development tsx --env-file=.env --watch ./api/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.29.3",
    "express": "~4.16.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.2.0",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cookie-session": "^2.0.48",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pg": "^8.10.9",
    "drizzle-kit": "^0.20.10",
    "express-generator": "^4.16.1",
    "prettier": "^3.1.1"
  }
}
